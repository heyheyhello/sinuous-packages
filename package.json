{
  "name": "sinuous-packages",
  "version": "0.0.0",
  "description": "Packages for Sinuous",
  "keywords": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/nthm/sinuous-packages"
  },
  "author": "Gen Hames",
  "scripts": {
    "build": "run-s babel terser",
    "babel": "babel --no-babelrc . --ignore 'node_modules' --out-dir dist --source-maps --extensions \".ts\" --plugins=@babel/plugin-transform-typescript",
    "terser": "run-p terser:*",
    "terser:lifecycle": "terser dist/sinuous-lifecycle/index.js --module --mangle --compress --source-map \"content='dist/sinuous-lifecycle/index.js.map'\" --output dist/sinuous-lifecycle/index.min.js",
    "terser:trace": "terser dist/sinuous-trace/index.js --module --mangle --compress --source-map \"content='dist/sinuous-trace/index.js.map'\" --output dist/sinuous-trace/index.min.js"
  },
  "dependencies": {
    "sinuous": "^0.27.6",
    "sinuous-trace": "file:sinuous-trace",
    "sinuous-lifecycle": "file:sinuous-lifecycle"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-transform-typescript": "^7.10.3",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-typescript": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": "^7.3.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.18.1",
    "rollup-plugin-size": "^0.2.2",
    "rollup-plugin-terser": "^6.1.0",
    "typescript": "^3.9.5"
  }
}
